{
  "name": "app-store",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/postgres": "^1.3.1",
    "@sap/cds": "^7",
    "express": "^4",
    "@sap/xssec": "^3",
    "passport": "^0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "start": "cds-serve",
    "build-src": "tsc -p tsconfig.json",
    "w": "concurrently \"tsc -w -p tsconfig.json\" \"cds w\"",
    "build": "mbt build -t gen --mtar mta.tar",
    "deploy": "cf deploy gen/mta.tar"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": "postgres",
        "auth": "xsuaa"
      },
      "[hybrid]": {
        "db": {
          "kind": "postgres",
          "impl": "@cap-js/postgres"
        },
        "auth": "xsuaa"
      },
      "db": {
        "kind": "postgres",
        "impl": "@cap-js/postgres"
      },
      "auth": "dummy"
    }
  }
}
