{
  "name": "launchpad",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "btp-cap-monorepo",
  "sourceRoot": "apps/launchpad/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/launchpad",
        "index": "apps/launchpad/src/index.html",
        "browser": "apps/launchpad/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/launchpad/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/launchpad/src/favicon.ico",
          "apps/launchpad/src/assets"
        ],
        "styles": [
          "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
          "apps/launchpad/src/styles.less",
          "apps/launchpad/src/styles.scss",
          {
            "input": "apps/launchpad/src/styles/default.less",
            "bundleName": "default",
            "inject": false
          },
          {
            "input": "apps/launchpad/src/styles/dark.less",
            "bundleName": "dark",
            "inject": false
          }
        ],
        "scripts": [],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/launchpad/src/styles/themes"]
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2000kb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "proxyConfig": "apps/launchpad/proxy.conf.json"
      },
      "configurations": {
        "production": {
          "buildTarget": "launchpad:build:production"
        },
        "development": {
          "buildTarget": "launchpad:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "launchpad:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/launchpad/**/*.ts",
          "apps/launchpad/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/launchpad/jest.config.ts"
      }
    }
  }
}
